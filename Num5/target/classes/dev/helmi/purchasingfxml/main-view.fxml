<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.2"
      fx:controller="dev.helmi.purchasingfxml.MainController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="welcomeText" style="-fx-font-size: 24;" text="Welcome"/>
    <Button onAction="#onVendorButtonClick" text="Vendor"/>
    <Button onAction="#onProductButtonClick" text="Product"/>
    <Button onAction="#onPembelianButtonClick" text="Pembelian"/>
    <Button onAction="#onExitButtonClick" text="Keluar"/>
</VBox>
